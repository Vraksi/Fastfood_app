<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" navigatingTo="navigatingTo">
    <ActionBar title="Names List" icon=""></ActionBar>
    <StackLayout style="background-color:white">
        <StackLayout orientation="horizontal">
        
        <Label text="Tap the button" class="h1 text-center" />
        <Button text="TAP" tap="{{ onTap }}" class="-primary" />
        <Label text="{{ message }}" class="h2 text-center" textWrap="true" />
            <Repeater items="{{ _Accessory }}">
                <Repeater.itemTemplate>
                    <StackLayout orientation="horizontal">
                        <Label text="{{ name }}" style="width:33%" />
                        <Label text=" {{ tag }}" style="width:33%" />
                        <Label text="{{ id }}" style="width:33%" />
                    </StackLayout>
                </Repeater.itemTemplate>
            </Repeater>
        </StackLayout>
    </StackLayout>
</Page>
